<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:orientation="vertical"
    tools:context="com.example.apurp_000.dementiaapp.MemoryWinPage" tools:deviceIds="wear_square"
    android:background="@color/white">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/imageButtonMemoryWinPage"
            android:layout_gravity="center"
            android:src="@drawable/memoryscorepage"
            android:contentDescription="MemoryWintButon"
            android:background="@color/white"/>

        <TextView
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="12"
            android:id="@+id/textView3"
            android:layout_gravity="right|center_vertical"
            android:textColor="@color/black" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="You Missed:"
            android:id="@+id/staticMissed"
            android:layout_gravity="left|center_vertical"
            android:textColor="@color/black" />
    </FrameLayout>
</LinearLayout>
